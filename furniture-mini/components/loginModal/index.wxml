<view class="cu-modal {{modalName=='DialogModal1'?'show':''}}">
	<view class="cu-dialog">
		<view class="cu-bar bg-white justify-end">
			<view class="content"></view>
			<view class="action" bindtap="hideModal">
				<text class="cuIcon-close text-red"></text>
			</view>
		</view>
		<view class="padding-xl">
			<image src="../../image/no-login.jpeg"></image>
		</view>
		<view class="cu-bar bg-white justify-end login-bottom">
			<view class="action">
				<button class="cu-btn line-green text-blue" bindtap="hideModal">取消</button>
				<button class="cu-btn bg-blue margin-left" open-type="getUserInfo" bindgetuserinfo="bindGetUserInfo">确定</button>
			</view>
		</view>
	</view>
</view>