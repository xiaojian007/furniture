page {
  height: 100%;
}
.pages {
  background: #f3f3f3;
  width: 100%;
  overflow-x: hidden;
}


/* 列表 */
.shop-item  .list {
  position: relative;
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  flex-direction: row;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(130rpx);
  transform: translateX(130rpx);
  margin-left: -130rpx;
}

/* 编辑 */
.cartHeader {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx 30rpx;
  border: 1px solid transparent;
  position: relative;
}

.cartHeader .logo image {
  width: 30rpx;
  height: 30rpx;
  display: inline-block;
  vertical-align: middle;
}

.cartHeader .logo text {
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
  font-family: NotoSansHans-Regular;
  font-weight: 400;
  color: rgba(48, 48, 48, 1);
  margin-left: 15rpx;
}

.edit {
  height: 60rpx;
  line-height: 70rpx;
  font-size: 24rpx;
}


/* 编辑标题 */
.edit_btn {
  position: absolute;
  font-size: 24rpx;
  color: #36A661;
  right: 30rpx;
}

/* 单选状态 */
.radio_chek {
  width: 40rpx;
  height: 40rpx;
}

/* 列表商品图片 */
.list_img {
  width: 180rpx;
  height: 180rpx;

}

/* 列表规格 */
.cart_g_name {
  font-size: 26rpx;
  color: #aaa;
  top: 75rpx;
}

/* 收藏 */
.collert {
  position: absolute;
  font-size: 28rpx;
  right: 40rpx;
  top: 30rpx;
}

/* 收藏图标 */
.collert_img {
  position: relative;
  top: 2rpx;
  width: 28rpx;
  height: 28rpx;
}

/* 列表商品名称 */
.list_name {
  width: 350rpx;
  /* background-color: red; */
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 列表商品价格 */
.list_price {
  font-size: 30rpx;
  color: #F0563C;
}

/* 列表商品删除 */
.list_del {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 44rpx;
}

/* 固定底部 */
.cont_bot {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 84rpx;
  line-height: 84rpx;
  background: #fff;
}

/* 全选ICON */
.total-select {
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 40rpx;
  height: 40rpx;
}

/* 全选标题 */
.sel_count_name {
  position: absolute;
  left: 90rpx;
  top: 0;
  font-size: 30rpx;
  color: #333;
}

/* 合计金额 */
.count_price {
  position: absolute;
  font-size: 28rpx;
  right: 250rpx;
  color: #aaa;
}

.count_price>view {
  height: 40rpx;
  line-height: 40rpx;
  color: #231D1E;
}

.count_price>view:first-child text {
  float: right;
  font-size: 20rpx;
  color: #5D5D5D;
}

.count_price view:last-child text {
  font-size: 30rpx;
  color: #F0563C;
}

/* 购物车为空 */
.list_none {
  padding: 40rpx 0;
  color: #999;
  text-align: center;
}

/* 提交 */
.submit {
  position: absolute;
  width: 214rpx;
  height: 84rpx;
  line-height: 84rpx;
  right: 0rpx;
  font-size: 30rpx;
  text-align: center;
  color: #fff;
  background-color: #3B7CF0;
  /* background: linear-gradient(90deg, rgba(123, 202, 110, 1) 0%, rgba(47, 163, 93, 1) 100%); */
}

.navigator-hover {
  background: none;
}




/*数量加减*/
.carts-num {
  position: absolute;
  right: 30rpx;
  bottom: 23rpx;
  /* background-color: red; */
  display: flex;
  align-items: center;
  text-align: center;
  height: 50rpx;
}

.view_text_center {
  font-size: 30rpx;
  display: inline-block;
  color: #333;
  height: 50rpx;
  line-height: 50rpx;
  width: 76rpx;
}

.carts-num .minus,
.carts-num .plus {
  margin: 10rpx;
  border: 1rpx solid #ebebeb;
  color: #000;
  text-align: center;
  height: 50rpx;
  line-height: 50rpx;
  width: 40rpx;
}

.carts-num .minus {
  font-size: 34rpx;
}

/* .carts-num .minus.disabled{color: red;} */
.carts-num .plus {
  font-size: 34rpx;
}


.flow-btn-min {
  background: #3B7CF0;
  height: 76rpx;
  line-height: 76rpx;
  font-size: 30rpx;
  width: 300rpx;
  margin: 0 auto;
  border-radius: 76rpx;
  text-align: center;
  color: #fff;
  margin-top: 25rpx;
}

.shop-item {
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
  display: flex;
}
.shop-item:last-child{
  margin-bottom: 124rpx;
}

.remove {
  background-color: #F0563C;
  width: 130rpx;
  display: flex;
  font-size: 26rpx;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(130rpx);
  transform: translateX(130rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}

.touch-move-active .list,
.touch-move-active .remove {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.goods-name .twolist-hidden {
  max-width: 300rpx;
}

.shop-radio {
  width: 80rpx;
  text-align: center;
}

.shop-img {
  width: 180rpx;
  margin-right: 20rpx;
}

.shop-goods {
  flex: 1;
  margin-right: 30rpx;
}

.dish-item-money {
  color: #ff495e;
  font-size: 32rpx;
}

.no-shop{
  background-image: url('data:image/png;base64,');
  background-size: 210rpx 160rpx;
  background-position: center center;
  background-repeat: no-repeat;
  width: 210rpx;
  height: 160rpx;
  margin: 50rpx auto 0;
}

.product-list{
  padding: 0 30rpx;
  background-color: #fff;
}

.product-list .home-title{
  color: #707070;
  font-size: 30rpx;
  padding: 38rpx 0;
}