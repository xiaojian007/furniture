/* pages/checkout/checkout.wxss */
.address-bg-back{
  height: 60rpx;
}
.address-bg {
  display: block;
  width: 100%;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #fff;
  background: #6899FF;
  position: fixed;
  top: 0;
  left: 0;
}

.address-box {
  display: flex;
  align-items: center;
  background-color: #fff;
  padding: 40rpx 14rpx 30rpx 140rpx;
  background-image: url('data:image/png;base64,');
  margin: 20rpx 30rpx;
  background-repeat: no-repeat;
  padding-left: 100rpx;
  background-position: 14rpx center;
  background-size: 69rpx 69rpx;
  border-radius: 4rpx;
  font-size: 30rpx;
  color: #000;
}
.address-box.add-address-box{
  background-color: #f3f3f3;
  margin-left: 50rpx;
  padding-right: 20rpx;
  background-image: url('data:image/png;base64,');
}
.add-adress{
  background-color: #3b7cf0;
  width: 180rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
}

.address-box .l .default{
  display: inline-block;
  font-size: 20rpx;
  color: #3b7cf0;
  margin-left: 16rpx;
  padding: 2rpx 8rpx;
  border: 2rpx solid #3b7cf0;
  border-radius: 20rpx;
}
.address-box .l .phone{
  margin-left: 16rpx;
  font-size: 28rpx;
}
.address-box .m {
  width: 550rpx;
  min-height: 84rpx;
}


.address-box .address {
  display: block;
  margin-top: 16rpx;
  color: #333;
  font-size: 24rpx;
}

.address-box .r image {
  width: 16rpx;
  height: 27rpx;
}

.cell {
  padding: 13px 15px !important;
}

.product {
  margin-top: 20rpx;
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
}

.product .item {
  height: 210rpx;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #ebebeb;
}

.product .item.no-border {
  border-bottom: none;
}

.product .item:last-child {
  border-bottom: none;
}

.product .img {
  height: 180rpx;
  width: 180rpx;
  /* background-color: #f4f4f4; */
  margin-right: 30rpx;
}

.product .img image {
  height: 180rpx;
  width: 180rpx;
}

.product .info {
  height: 180rpx;
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: space-between;
}

.product .t {
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #555;
}

.product .t .name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 300rpx;
}

.product .m {
  font-size: 25rpx;
  color: #999;
  white-space: nowrap;
  width: 300rpx;
  text-overflow: ellipsis;
}

.product .prices {
  display: flex;
}

.product .b {
  font-size: 28rpx;
  color: #555;
}

.product .originPrice {
  font-size: 26rpx;
  color: #999;
  margin-left: 10rpx;
  text-decoration: line-through;
}

.agreement {
  display: flex;
  padding-left: 32rpx;
  height: 130rpx;
  align-items: center;
}

.checked {
  color: #333;
  font-size: 26rpx;
}

.bottom {
  position: fixed;
  bottom: 0;
  height: 94rpx;
  width: 750rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1rpx solid #ebebeb;
  background: #fff;
}

.bottom .l {
  font-size: 30rpx;
  padding-left: 32rpx;
  color: #F0563C;
}

.bottom .r {
  width: 203rpx;
  height: 94rpx;
  background: #3B7CF0;
  line-height: 94rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
}

.bottom .r.active {
  background: #3B7CF0;
}

.bottom .r.disabled {
  background: #ccc !important;
}

.check-product {
  margin: 20rpx 30rpx 114rpx;
  background: #fff;
  padding: 14rpx 14rpx 0;
  border-radius: 4rpx;
}

.product-order .weui-cell {
  padding: 30rpx 0;
  font-size: 24rpx;
  color: #333;
}
.product-order .weui-cell::before{
  left: 0;
}

.product-order .weui-cell .out-right {
  color: #000;
  text-align: right;
  flex: 1;
}



.product-order .weui-cell .out-right .price {
  font-size: 26rpx;
  color: #DF5E49;
}

.product-order .weui-cell .out-right .cal {
  font-size: 20rpx;
}
.check-title{
  font-size: 30rpx;
  font-weight: bold;
  color: #000;
  padding: 10rpx 0 16rpx;
}